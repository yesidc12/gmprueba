		TERRITORIOS_INFO[i][Territorio_BandaID2] = 		TERRITORIOS_INFO[i][Territorio_BandaID];
		if(TERRITORIOS_INFO[i][Territorio_Banda] == 0) 	TERRITORIOS_INFO[i][Territorio_Usado] = false;
