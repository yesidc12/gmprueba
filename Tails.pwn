		TERRITORIOS_INFO[i][Territorio_AtacanteID] = 0;
		
		if(gang_color == 0) TERRITORIOS_INFO[i][Territorio_Color] = 0x252b2e55;
		else
		{
			new r, g, b, a;
			HexToRGBA(gang_color, r, g, b, a);
			TERRITORIOS_INFO[i][Territorio_Color] = RGBAToHex(r, g, b, 135);
