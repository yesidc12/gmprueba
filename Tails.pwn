				format(chatadmin,sizeof(chatadmin),"{%06x}[Radio] {FFFFFF}%s%s dice: %s",BandasInfo[PlayerInfo[playerid][pFaccion]][BColor] >>> 8, rango,PlayerInfo[playerid][Nickname],text[1]);
