		if(TERRITORIOS_INFO[i][Territorio_EnGuerra])
		{
			new flash_color, r, g, b, a;
			HexToRGBA(BandasInfo[TERRITORIOS_INFO[i][Territorio_AtacanteID]][BColor], r, g, b, a);
