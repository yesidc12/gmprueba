	    DetectoSPAM(playerid, text);
	}
	if(text[0] == '!' && text[1] != '!')
	{
		if(PlayerInfo[playerid][pTiempoCarcel] > 0 || PlayerInfo[playerid][pTiempoJail] > 0)
		{
			SendClientMessage(playerid,-1,"Te quitaron el radio comunicador antes de meterte en la celda.");
