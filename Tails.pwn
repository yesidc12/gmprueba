funcion DudaUser(playerid, const params[])
{
	if(sscanf(params, "s[300]", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Escribe {7401DF}/n [Pregunta]{FFFFFF} para preguntar/responder a otros jugadores");
	{
		new string[300];
