    FABRICAS_INFO[ID][f_DINERO] -= cantidad;
    if(FABRICAS_INFO[ID][f_DINERO] < 0) FABRICAS_INFO[ID][f_DINERO] = 0;
	ActualizarDineroFabrica(ID);
	return 1;
}
DarDineroFabrica(ID, cantidad)
{
    if(!FABRICAS_INFO[ID][f_VALID]) return 1;
    FABRICAS_INFO[ID][f_DINERO] += cantidad;
    if(FABRICAS_INFO[ID][f_DINERO] < 0) FABRICAS_INFO[ID][f_DINERO] = 0;
	ActualizarDineroFabrica(ID);
	return 1;
}

