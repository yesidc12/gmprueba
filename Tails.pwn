				if(Reanimado[x] == 0 && RopaMedi[x] == 1 && (PlayerInfo[x][pTrabajo] == T_Medico || PlayerInfo[x][pTrabajo2] == T_Medico))
				{
					GetPlayerPos(x, DeathXH[x],DeathYH[x],DeathZH[x]);
					SetPlayerMarkerForPlayer(x, playerid, COLORABATIDO);
				}
			}
		}
