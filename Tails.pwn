	{
		PLAYER_TIMES[playerid][TiempoAcademia]++;
	}
	if(YoLlame[playerid] == 1) MinutoLlamada[playerid] += 1;
	
	if(PLAYER_TIMES[playerid][TiempoAgresor] > 0 && Agresor[playerid] != INVALID_PLAYER_ID)
	{
		PLAYER_TIMES[playerid][TiempoAgresor]--;
	}
