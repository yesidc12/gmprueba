				SendClientMessage(playerid,-1,"{FFFFFF}Escribe {7401DF}/cerrar {FFFFFF}para trabar las puertas de tu vehículo.");
			}
		}
		else if(IsPlayerInRangeOfPoint(playerid, 5.0, x2,y2,z2))
		{
		    //if(PlayerInfo[playerid][v2Traba] == 0) return SendClientMessage(playerid, COLOR_WHITE, "Para poder abrir el vehículo, necesitas instalar un sistema de traba. Puedes comprar uno en cualquier {7401DF}24/7");
			if(bloqueado[autos2[playerid]] == 1)
			{
