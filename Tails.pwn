        GetPlayerFacingAngle(playerid, bpos[playerid][3]); bpos[playerid][3] = bpos[playerid][3] +180;
        parlanteObject[playerid] = CreateDynamicObject(2231, bpos[playerid][0], bpos[playerid][1], bpos[playerid][2], 0, 0, bpos[playerid][3]);
        format(parlanteURL[playerid], 256, "%s", params);
		format(string, sizeof(string), "Parlante de %s\n",PlayerName(playerid));
		LABELPARLANTE[playerid] = CreateDynamic3DTextLabel(string, -1, bpos[playerid][0], bpos[playerid][1], bpos[playerid][2]+0.6, 5, .worldid = GetPlayerVirtualWorld(playerid), .interiorid = GetPlayerInterior(playerid));
  		timerparlante[playerid]= SetTimerEx("quitarparlante",1200000,false,"d",playerid);
	}
    else
    {
        parlante[playerid] = 0;
		KillTimer(timerparlante[playerid]);
        format(string, sizeof(string), "{00AEFF}[Parlante]:{FFFFFF} Has removido tu parlante.");
        ApplyAnimation(playerid, "BOMBER", "BOM_Plant", 4.0, 0, 1, 1, 0,0);
        SendClientMessage(playerid, 0x33CC99FF, string);
        DestroyDynamicObject(parlanteObject[playerid]);
       	DestroyDynamic3DTextLabel(LABELPARLANTE[playerid]);
        format(parlanteURL[playerid], 256, "");
        foreach(new i : Player)
        {
            if(IsPlayerConnected(i))
            {
                if(parlantePlayer[i] == playerid)
                {
                    parlanteStream[i] = 0;
                    parlantePlayer[i] = -1;
                    StopAudioStreamForPlayer(i);
                    SendClientMessage(i, COLOR_GREY, "{00AEFF}[Parlante]:{FFFFFF} El dueño de la música removió el parlante que estabas escuchando.");
                }
            }
        }
    }
    return 1;
}
