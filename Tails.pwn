		PLAYER_TEMP[playerid][p_SELECTED_TOY_SLOT] = -1;
	}
	else
	{
	    RemovePlayerAttachedObject(playerid, PLAYER_TEMP[playerid][p_SELECTED_TOY_SLOT]);
		if(PLAYER_TOYS[playerid][PLAYER_TEMP[playerid][p_SELECTED_TOY_SLOT]][pt_ATTACHED])
