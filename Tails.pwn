public CambiarNumero(playerid)
{
	if(cache_num_rows() == 1)
	{
	 SendClientMessage(playerid,-1,"Este número ya se encuentra en uso, intenta con otro.");
  	 ShowPlayerDialog(playerid,D_CONFIGNU, DIALOG_STYLE_INPUT,"{FFFF00}Cambiar número","{FFFFFF}Ingresa el número que quieres usar:","Ingresar","Salir");
	 return 1;
	}
	new stringg[200],precionum;
	precionum = 10;
	if(PlayerInfo[playerid][pNumero] > 100 && PlayerInfo[playerid][pNumero] < 1000) precionum = 40;
	if(PlayerInfo[playerid][pNumero] > 1000 && PlayerInfo[playerid][pNumero] < 10000) precionum = 35;
	if(PlayerInfo[playerid][pNumero] > 10000 && PlayerInfo[playerid][pNumero] < 100000) precionum = 15;
	if(PlayerInfo[playerid][pNumero] > 100000) precionum = 10;
 	format(stringg,sizeof(stringg),"{FFFFFF}Cambiar número de teléfono\n           %d ---> {89746A}%d{FFFFFF}\n           Costo: {00CCFF}%d"SERVER_COIN"{FFFFFF}.",PlayerInfo[playerid][pNumero],PlayerInfo[playerid][pNumero],precionum);
 	ShowPlayerDialog(playerid, D_CONFIGNU2, DIALOG_STYLE_MSGBOX, "{FFFF00}Cambiar número", stringg, "Si","No");
	return 1;
}

