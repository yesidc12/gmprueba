CMD:tw(playerid, params[])
{
  new dinero = GetPlayerMoney(playerid);
  if(dinero >= 1000) // Aqui colocan la misma cantidad de "GivePlayerMoney" de abajo, pero en positivo.
  {
    new Mensaje[32], Name[24], Formato[64]; GetPlayerName(playerid, Name, sizeof(Name));
    if(sscanf(params, "s[32]", Mensaje)) return SendClientMessage(playerid, (0x33CCFFFF), "/tw [mensaje]"); //donde dice "/tw [mensaje]" es para explicar al usuario como debe usar el CMD.
    format(Formato, sizeof(Formato), "-Twitter | @%s: %s", Name, Mensaje);
    SendClientMessageToAll(0x33CCFFFF, Formato);
    GivePlayerMoney(playerid, -1000);   //donde dice "-1000" es el dinero que costara enviar un twitter.
  } else return SendClientMessage(playerid, COLOR_WHITE, "[ERROR]"#rojo" Necesitas 1000 dolares para usar twitter"); //Aqui debes editar el mensaje que saldra cuando no tengas el dinero suficiente.
  return 1;
}

