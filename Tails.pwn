		new info[2];
		info[0] = AREA_TYPE_GANGZONE;
		info[1] = i;
		Streamer_SetArrayData(STREAMER_TYPE_AREA, TERRITORIOS_INFO[i][Territorio_Area], E_STREAMER_EXTRA_ID, info);
		territoriosn ++; total++;
	}
	printf("[TERRITORIOS]: %d",total);
	return 1;
