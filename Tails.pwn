		sscanf(string, "p<,>dddddddddd", DINERO_STATS, NIVEL_STATS, MONEDAS_STATS, CRACK_STATS, REMEDIOS_STATS, GPS_STATS, CELL_PHONE_STATS, ARMERO_STATS, MATERIALES_STATS,LADRON_STATS);
