ColocoSi_No(const text[])
{
    new len = strlen(text);
    for(new i = 2; i < len; i++)
    {
        if(text[i-1] == ' ' && text[i] == 's' && text[i+1] == 'i') return 1;
        else if(text[i-1] == ' ' && text[i] == 'S' && text[i+1] == 'i') return 1;
        else if(text[i-1] == ' ' && text[i] == 's' && text[i+1] == 'I') return 1;
        else if(text[i-1] == ' ' && text[i] == 'S' && text[i+1] == 'I') return 1;
        else if(text[i-1] == ' ' && text[i] == 'n' && text[i+1] == 'o') return 1;
        else if(text[i-1] == ' ' && text[i] == 'N' && text[i+1] == 'o') return 2;
        else if(text[i-1] == ' ' && text[i] == 'n' && text[i+1] == 'O') return 2;
        else if(text[i-1] == ' ' && text[i] == 'N' && text[i+1] == 'O') return 2;
    }
	return 0;
}

