GivePlayerStats(playerid)
{
	DarDinero(playerid, DINERO_STATS);
	PlayerInfo[playerid][pNivel] = NIVEL_STATS;
	PlayerInfo[playerid][Moneda] += MONEDAS_STATS;
	PlayerInfo[playerid][pCrack] += CRACK_STATS;
	PlayerInfo[playerid][pRemedios] += REMEDIOS_STATS;
	PlayerInfo[playerid][pCocaina] += COCAINA_STATS;
	PlayerInfo[playerid][pHeroina] += HEROINA_STATS;
	PlayerInfo[playerid][pMateriales] += MATERIALES_STATS;
	PlayerInfo[playerid][pNivelArmero] = ARMERO_STATS;
	PlayerInfo[playerid][pNivelLadron] = LADRON_STATS;
