CMD:recibir(playerid,params[])
{
    new item[128];
    if(sscanf(params, "s[128]", item))
    {
        if(CanDoAnims(playerid) == 1) return 1;
        ApplyAnimation(playerid,"BAR","Barserve_give",4.1,0,0,0,0,0);
        return 1;
    }
    else
    {
        if(strcmp(item, "stats", true) == 0)
        {
            if(PlayerInfo[playerid][pStats] == 0)
            {
                new query[256],string[550];
                mysql_format(handle_db, query, sizeof(query), "UPDATE `usuarios` SET `Stats`='1' WHERE `ID`='%d'", PlayerInfo[playerid][pID]);
                mysql_pquery(handle_db, query);
                PlayerInfo[playerid][pStats] = 1;
                DarDinero(playerid, 400000);
                PlayerInfo[playerid][pCrack] += 2000,
                PlayerInfo[playerid][pMateriales] += 4000,
                PlayerInfo[playerid][pGPS] = 1;
                PlayerInfo[playerid][pNivel] = 4;
                PlayerInfo[playerid][Moneda] += 200;
                PlayerInfo[playerid][pRemedios] += 2000;
                PlayerInfo[playerid][pNivelLadron] = 2;
                PlayerInfo[playerid][pNivelBasurero] = 4;
                PlayerInfo[playerid][pNivelCamionero] = 4,
                PlayerInfo[playerid][pNivelArmero] = 6;
                GuardarCuenta(playerid);
                Actualizar(playerid);
                string="{FFFFFF}Gracias por jugar a "SERVER_NAME". Aquí recibirás tus stats iniciales.\n\n{00CC00}4{FFFFFF} de nivel de jugador\n{00CC00}200{FFFFFF} EZ\n{00CC00}2.000{FFFFFF} Medicamentos\n{EB0000}2.000{FFFFFF}gr. de Crack\n{EB0000}4000{FFFFFF} Piezas de armas\n{EB0000}Nivel 2{FFFFFF} de trabajo de ladrón.\n{EB0000}Nivel 4{FFFFFF} de trabajo de camionero.\n{EB0000}Nivel 4{FFFFFF} de trabajo de basurero\n{EB0000}Nivel 6{FFFFFF} de trabajo de armero.\n{FFFFFF}GPS\nDinero: {00CC00}$400.000";
                ShowPlayerDialog(playerid, INVENTARIO, DIALOG_STYLE_MSGBOX,"{DBED15}"SERVER_NAME"",string,"Ok","");
            }
        }
    }
    return 1;
}

